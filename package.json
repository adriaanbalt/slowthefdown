{
  "name": "SlowtheFdown",
  "version": "1.0.0",
  "description": "SlowtheFdown is a game where you try to slow the F down.",
  "engines": {
    "node": "7.10.0",
    "npm": "4.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adriaanbalt/slowthefdown.git"
  },
  "config": {
    "destination_dir": "public",
    "source_dir": "source",
    "build_scripts_dir": "bin"
  },
  "scripts": {
    "server": "node server/index.js",
    "client": "webpack-dev-server -d --inline --hot --config $npm_package_config_build_scripts_dir/webpack/webpack.dev.config.js --watch --progress",
    "client-prod": "webpack --config $npm_package_config_build_scripts_dir/webpack/webpack.prod.config.js",
    "prod": "NODE_ENV=production concurrently \"npm run server\" \"npm run client-prod\"",
    "start": "concurrently \"npm run client\" \"npm run server\""
  },
  "dependencies": {
    "abbrev": "^1.0.7",
    "babel-core": "^6.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.4.3",
    "bluebird": "^3.2.2",
    "body-parser": "^1.15.0",
    "classnames": "^2.1.3",
    "compass": "^0.1.1",
    "compression": "^1.6.2",
    "concurrently": "^3.5.0",
    "connect-mongo": "^1.1.0",
    "cookie-parser": "^1.4.0",
    "cors": "^2.7.1",
    "css-loader": "^0.28.4",
    "del": "^1.1.1",
    "dotenv": "^2.0.0",
    "express": "^4.13.3",
    "express-session": "^1.13.0",
    "http-proxy": "^1.11.2",
    "lodash": "^3.5.0",
    "lodash.get": "^3.7.0",
    "lodash.set": "^3.7.4",
    "merge-stream": "^0.1.7",
    "mongoose": "~4.1.6",
    "morgan": "^1.6.1",
    "node-refills": "^1.0.1",
    "node-sass": "^4.5.3",
    "npmlog": "^2.0.0",
    "object-assign": "^2.1.1",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.0",
    "passport-google-oauth": "^1.0.0",
    "passport-local": "^1.0.0",
    "react": "^15.0.0",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.3.1",
    "redux-devtools": "^3.1.1",
    "redux-devtools-dock-monitor": "^1.1.0",
    "redux-devtools-log-monitor": "^1.0.5",
    "redux-thunk": "^2.0.1",
    "require-dir": "^0.1.0",
    "rimraf": "^2.5.2",
    "sass-loader": "^6.0.6",
    "semver": "^5.0.3",
    "serve-favicon": "^2.3.0",
    "shortid": "^2.2.4",
    "simplex-noise": "^2.2.0",
    "style-loader": "^0.18.2",
    "superagent": "^1.3.0",
    "three": "^0.76.1",
    "webpack": "^2.0.0",
    "webpack-dev-middleware": "^1.6.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "devDependencies": {
    "webpack-dev-server": "^2.5.1"
  }
}

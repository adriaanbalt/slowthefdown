{
  "name": "SlowtheFdown",
  "version": "1.0.0",
  "description": "SlowtheFdown is a game where you try to slow the F down.",
  "engines": {
    "node": "7.10.0",
    "npm":"3.10.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adriaanbalt/slowthefdown.git"
  },
  "config": {
    "destination_dir": "public",
    "source_dir": "source",
    "build_scripts_dir": "bin"
  },
  "scripts": {
    "server": "node server/index.js",
    "client": "webpack-dev-server -d --inline --hot --config $npm_package_config_build_scripts_dir/webpack/webpack.dev.config.js --watch --progress",
    "client-prod": "webpack --config $npm_package_config_build_scripts_dir/webpack/webpack.prod.config.js",
    "prod": "NODE_ENV=production concurrently \"npm run server\" \"npm run client-prod\"",
    "start": "concurrently \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "bluebird": "^3.2.2",
    "body-parser": "^1.15.0",
    "classnames": "^2.1.3",
    "compression": "^1.6.2",
    "concurrently": "^3.5.0",
    "connect-mongo": "^1.1.0",
    "cookie-parser": "^1.4.0",
    "cors": "^2.7.1",
    "css-loader": "^0.28.4",
    "dotenv": "^2.0.0",
    "express": "^4.13.3",
    "express-session": "^1.15.4",
    "http-proxy": "^1.11.2",
    "lodash": "^3.5.0",
    "lodash.get": "^3.7.0",
    "lodash.set": "^3.7.4",
    "mongoose": "^4.1.6",
    "morgan": "^1.6.1",
    "node-sass": "^4.5.3",
    "object-assign": "^2.1.1",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.0",
    "passport-google-oauth": "^1.0.0",
    "passport-local": "^1.0.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^6.0.6",
    "semver": "^5.0.3",
    "serve-favicon": "^2.3.0",
    "shortid": "^2.2.4",
    "simplex-noise": "^2.2.0",
    "style-loader": "^0.18.2",
    "superagent": "^1.3.0",
    "three": "^0.76.1",
    "webpack": "^3.1.0"
  },
  "devDependencies": {
    "webpack-dev-server": "^2.5.1"
  }
}
